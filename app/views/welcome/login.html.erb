<div>
	<h1 class="masthead text-center bigtitle">Orasi Coding Challenge</h1>
</div>
<div>
	<%= form_tag sessions_path do %>
	<p>
		<%= label_tag :username %>
		<br />
		<%= text_field_tag :username, params[:username] %>
	</p>
	<p>
		<%= label_tag :password %>
		<br />
		<%= password_field_tag :password %>
	</p>
	<p class="button">
		<%= submit_tag "Log in", class: "btn btn-primary btn-lg" %>
	</p>
	<%= link_to "New user?", 'users/new' %>
	<% end %>
</div>
