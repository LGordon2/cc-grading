<h1>Results</h1>
<p>
	<script type='text/vbscript'>
	<%= @uploaded_file_text.html_safe %>
	<% Dir.entries('res/'+params[:controller]).each do |file| %>
    <% if(File.directory?(file)==false) %>
    <%= File.read('res/'+params[:controller]+"/"+file).html_safe %>
	<% end %> 
	<% end %>
	</script>
</p>
<%= link_to('Submit again',root_path)%>
